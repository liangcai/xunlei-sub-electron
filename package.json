{
  "name": "xunlei-sub-electron",
  "version": "0.1.0",
  "private": true,
  "main": "electron.js",
  "homepage": ".",
  "build": {
    "appId": "com.xx.app",
    "extends": null,
    "asar": false,
    "files": [
      "build/**/*",
      "electron.js",
      "preload.js",
      ".env",
      "pydist/app/*"
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "antd": "^4.1.2",
    "axios": "^0.19.2",
    "create-react-app": "^3.4.1",
    "dotenv": "^8.2.0",
    "electron-dl": "^3.0.0",
    "moment": "^2.24.0",
    "python-shell": "^1.0.8",
    "qs": "^6.9.3",
    "react": "^16.13.1",
    "react-dev-utils": "^10.0.0",
    "react-dom": "^16.13.1",
    "react-dropzone": "^10.2.2",
    "react-scripts": "^3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev:win": "chcp 65001 && cross-env NODE_ENV=development electron .",
    "dev": "cross-env NODE_ENV=development electron .",
    "prepackage:win": "npm run build && npm run build-python",
    "package:win": "electron-builder --win --x64",
    "prepackage:mac": "npm run build && npm run build-python",
    "package:mac": "electron-builder --mac",
    "build-python": "pyinstaller -y ./api/app.py --clean --distpath ./pydist"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "js-file-download": "^0.4.11"
  }
}
